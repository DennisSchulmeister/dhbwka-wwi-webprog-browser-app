        "start": "parcel src/*.html"
    },
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-runtime": "^6.26.0",
        "highlight.js": "^9.12.0",
        "mini-tutorial.js": "^1.0.7",
        "parcel-bundler": "^1.9.7",
        "shelljs": "^0.7.8"
    }

    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/DennisSchulmeister/dhbwka-wwi-webprog-browser-app.git"
    },
    "keywords": [
        "mini-tutorial.js",
        "template",
        "documentation",
        "manual"
    ],
    "scripts": {
        "clean": "node scripts/clean.js",
        "build": "npm run clean; parcel build src/*.html --public-url . --detailed-report --experimental-scope-hoisting",
